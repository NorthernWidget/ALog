pan><span class="crayon-o">=</span><span class="crayon-h"> </span><span class="crayon-cn">0</span><span class="crayon-sy">;</span><span class="crayon-h"> </span><span class="crayon-c">//Valid range is 0-23 hours</span></div><div class="crayon-line crayon-striped-line" id="crayon-593141e2c52dc078587911-18"><span class="crayon-t">bool</span><span class="crayon-h"> </span><span class="crayon-v">external_interrupt</span><span class="crayon-h"> </span><span class="crayon-o">=</span><span class="crayon-h"> </span><span class="crayon-t">false</span><span class="crayon-sy">;</span><span class="crayon-h"> </span><span class="crayon-c">// e.g., rain gage</span></div><div class="crayon-line" id="crayon-593141e2c52dc078587911-19">&nbsp;</div><div class="crayon-line crayon-striped-line" id="crayon-593141e2c52dc078587911-20"><span class="crayon-t">void</span><span class="crayon-h"> </span><span class="crayon-e">setup</span><span class="crayon-sy">(</span><span class="crayon-sy">)</span><span class="crayon-sy">{</span><span class="crayon-h">&nbsp;&nbsp;</span><span class="crayon-c">//Serial baud rate is set to 38400</span></div><div class="crayon-line" id="crayon-593141e2c52dc078587911-21"><span class="crayon-h">&nbsp;&nbsp;</span><span class="crayon-v">logger</span><span class="crayon-sy">.</span><span class="crayon-e">initialize</span><span class="crayon-sy">(</span><span class="crayon-v">dataLoggerName</span><span class="crayon-sy">,</span><span class="crayon-h"> </span><span class="crayon-v">fileName</span><span class="crayon-sy">,</span></div><div class="crayon-line crayon-striped-line" id="crayon-593141e2c52dc078587911-22"><span class="crayon-h">&nbsp;&nbsp;&nbsp;&nbsp;</span><span class="crayon-v">Log_Interval_Hours</span><span class="crayon-sy">,</span><span class="crayon-h"> </span><span class="crayon-v">Log_Interval_Minutes</span><span class="crayon-sy">,</span><span class="crayon-h"> </span><span class="crayon-v">Log_Interval_Seconds</span><span class="crayon-sy">,</spa